{
  "name": "@liquid-js/qrcode-generator",
  "version": "1.0.0-2",
  "description": "QR Code Generator implementation in JavaScript.",
  "type": "module",
  "main": "./lib/qr-code.js",
  "types": "./lib/qr-code.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./lib/qr-code.d.ts",
      "default": "./lib/qr-code.js"
    }
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/jsdom": "^21.1.7",
    "@types/node": "<21.0.0",
    "@types/sharp": "^0.32.0",
    "clean-package": "^2.2.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "npm-check-updates": "^17.1.4",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.10",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "test": "jest",
    "lint": "prettier -w . && eslint . --fix",
    "prepublishOnly": "npm run build",
    "prepare": "husky",
    "release": "git diff --exit-code && standard-version && git push --follow-tags origin",
    "docs": "typedoc",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liquid-JS/qrcode-generator.git"
  },
  "keywords": [
    "qr",
    "qrcode",
    "generator"
  ],
  "author": "HitkoDev <development@hitko.si>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Liquid-JS/qrcode-generator/issues"
  },
  "homepage": "https://styled-qr.liquidjs.io/",
  "packageManager": "yarn@4.5.0",
  "clean-package": {
    "remove": [
      "packageManager",
      "scripts",
      "devDependencies",
      "clean-package"
    ]
  }
}
